{
  "name": "vite-react-typescript-eslint-prettier",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^20",
    "pnpm": "^8"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --quiet --cache --report-unused-disable-directives src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --cache --report-unused-disable-directives src/**/*.{ts,tsx,js,jsx}",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
